<%
const cfg = { ...require("./data.json"), ...arguments?.[0] }
const {
	extraClasses,
	extraAttrs,
	sitekey
} = cfg
%>

<%
const classes = `g-recaptcha ${extraClasses.join(" ")}`.trim()
const attrs = extraAttrs.join(" ")
%>

<div
	class="<%= classes %>"
	<%- extraAttrs %>
		data-js-recaptcha
	data-sitekey="<%= sitekey %>"
>
</div>
